  "type": "project",
  "config": {
    "preferred-install": "source",
    "vendor-dir": "private/packages",
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": false,
  "require": {
    "php": ">=5.4.0",
    "wikimedia/composer-merge-plugin": "dev-master",
    "selene-framework/framework": "dev-master"
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "composer.root.json",
        "private/modules/*/*/composer.json"
      ],
      "recurse": false,
      "merge-extra": false
    }
  },
  "autoload": {
    "files": ["private/bootstrap.php"]
  },
  "scripts": {
    "post-install-cmd": [
      "Selene\\CoreTasks::runInit"
    ],
    "post-update-cmd": [],
    "post-create-project-cmd": []
  }
}
