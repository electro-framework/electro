{
  "name": "selene-framework/starter-project",
  "description": "Selene Framework Starter Project",
  "keywords": [
    "application",
    "selene framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Impactwave",
      "email": "impactwave@impactwave.com",
      "homepage": "http://impactwave.com"
    },
    {
      "name": "ClÃ¡udio Silva",
      "email": "claudio.silva@impactwave.com"
