{
  "name": "electro/electro",
  "description": "The Electro PHP Web Framework",
  "keywords": [
    "framework"
  ],
  "license": "MIT",
  "homepage": "https://github.com/electro/electro",
  "authors": [
    {
      "name": "Impactwave",
      "email": "impactwave@impactwave.com",
      "homepage": "http://impactwave.com"
    },
    {
      "name": "Cl√°udio Silva",
      "email": "claudio.silva@impactwave.com"
    }
  ],
  "type": "project",
  "config": {
    "preferred-install": {
      "electro/*": "source",
      "electro-modules/*": "source",
      "selenia/*": "source",
      "selenia-modules/*": "source",
      "php-kit/*": "source",
      "*": "dist"
    },
    "vendor-dir": "private/packages",
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "electro/framework": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "private/src"
    }
  },
  "scripts": {
    "pre-update-cmd": "bin/install go",
    "post-create-project-cmd": "App\\Bootloader::runInitCommand",
    "post-install-cmd": "App\\Bootloader::runInitCommand",
    "post-update-cmd": "App\\Bootloader::runUpdateCommand",
    "pre-package-uninstall": "App\\Bootloader::runUninstallCommand"
  },
  "repositories": [
    {
      "type": "path",
      "url": "private/packages/electro/framework/subsystems/*"
    },
    {
      "type": "path",
      "url": "private/packages/electro/framework/profiles/*"
    },
    {
      "type": "path",
      "url": "private/modules/*/*"
    }
  ]
}
